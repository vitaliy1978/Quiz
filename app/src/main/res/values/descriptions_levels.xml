    <string name="levelfour">Ещё одно лёгкое задание.</string>
    <string name="levelfour2">Выбери часы, которые\nпоказывают большее\nвремя.</string>
    <string name="levelfive">Приглядись.</string>
    <string name="levelfive2">Выбери тот цвет,\nкоторый темнее.</string>
    <string name="levelsix">Определи</string>
    <string name="levelsix2">что тяжелее?</string>
    <string name="levelseven">Выбери.</string>
    <string name="levelseven2">У кого больше\nгрузоподъёмность?</string>
    <string name="leveleight"></string>
    <string name="leveleight2">Кто из них быстрее?</string>
